{
    "name": "req-query-cache",
    "version": "0.1.0",
    "description": "Rails-style per-request and optional cross-request query caching for Node.js (Express & NestJS), TypeScript-friendly.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "cache",
        "query",
        "mongodb",
        "postgresql",
        "sql",
        "nosql",
        "typescript",
        "nestjs",
        "express",
        "async-local-storage"
    ],
    "author": "Ali nazari backendwithali@gmail.com",
    "license": "MIT",
    "type": "module",
    "imports": {
        "#app/*": {
            "my-package-dev": "src/*",
            "default": "./dist/*"
        }
    },
    "scripts": {
        "tsc": "tsc",
        "dev": "tsx --watch src/*.ts",
        "prebuild": "npm run tsc",
        "build": "tsup",
        "prestart": "npm run build",
        "start": "node dist/index.js",
        "test": "vitest run --coverage",
        "test:watch": "vitest dev",
        "clean": "del-cli dist/ coverage/ *.log"
    },
    "peerDependencies": {
        "@nestjs/common": "*",
        "@nestjs/core": "*",
        "express": "*"
    },
    "devDependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@types/express": "^5.0.2",
        "@types/node": "^22.13.10",
        "@vitest/coverage-v8": "^3.0.9",
        "del-cli": "^6.0.0",
        "tsup": "^8.4.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vitest": "^3.0.9"
    }
}
